<p id="notice"><%= notice %></p>

<h1>Listing Digital Signatures</h1>

<table>
  <thead>
    <tr>
      <th>Image</th>
      <th>Intent</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @digital_signatures.each do |digital_signature| %>
      <tr>
        <td><%= digital_signature.image %></td>
        <td><%= digital_signature.intent %></td>
        <td><%= link_to 'Show', digital_signature %></td>
        <td><%= link_to 'Edit', edit_digital_signature_path(digital_signature) %></td>
        <td><%= link_to 'Destroy', digital_signature, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Digital signature', new_digital_signature_path %>
